#!/bin/bash -e
DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && cd .. && pwd )
LIBS=${DIR}/usr/lib
export MAGICK_CODER_MODULE_PATH=$(echo ${DIR}/usr/lib/ImageMagick-*/modules-Q*/coders)
export MAGICK_CONFIGURE_PATH=$(echo $DIR/etc/ImageMagick-*)
exec ${DIR}/lib/ld-*.so* --library-path $LIBS ${DIR}/usr/bin/identify "$@"
